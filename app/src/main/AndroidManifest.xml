<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="quintacarmelita.com">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="quintacarmelita.com.ProfileActivity"
            android:label="@string/title_activity_log"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="quintacarmelita.com.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="quintacarmelita.com.RegisterActivity" />
        <activity android:name="quintacarmelita.com.SignedInActivity" />
        <activity android:name="quintacarmelita.com.ProfileCreateActivity" />
        <activity android:name="quintacarmelita.com.ProfileMenuActivity" />
        <activity android:name="quintacarmelita.com.ProfileDatosGenerales" />
        <activity android:name="quintacarmelita.com.ProfilePerfilMedico" />
        <activity android:name="quintacarmelita.com.ProfileBitacora" />

    </application>

</manifest>