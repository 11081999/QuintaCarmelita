        android:hint="Incidentos o Eventos"
        app:layout_constraintBottom_toTopOf="@+id/Bit4"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/Bit4"
        android:layout_width="282dp"
